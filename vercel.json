{
    "version": 2,
    "framework": "django",
    "functions": {
        "api/index.py": {
            "runtime": "python3.9"
        }
    },
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "myapp.settings",
        "PYTHON_VERSION": "3.9"
    }
}